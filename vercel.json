{
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "./src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}